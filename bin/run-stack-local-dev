#!/usr/bin/env sh
# DOCKERBLOB="docker-compose -f docker-compose.yml -f docker-compose.override.yml -f docker-compose.dev-server.yml"
DOCKERBLOB="docker-compose -f docker-compose.dev-server.yml"
$DOCKERBLOB rm -fv
$DOCKERBLOB build
if [ $# -eq 1 ]
	then
		COMPUTE_CONFIG=`cat $1` $DOCKERBLOB up
	else
		$DOCKERBLOB up
fi
